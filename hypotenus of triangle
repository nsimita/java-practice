import java.util.*;
//hypotenus of triangle
public class Main
{
	public static void main(String[] args) {
	  double x;
	  double y; double z;
	  Scanner s = new Scanner(System.in);
	  System.out.println("input the size of x: ");
	  x = s.nextDouble();
	   System.out.println("input the size of y: ");
	  y = s.nextDouble();
	  z = Math.sqrt((x*x)+(y*y));
	  System.out.println(" the hypotenus is : "+z);
	  //Scanner.close(); //not necessary
	  
	  
	}
}
